.tpl-login .cnr{width:138px;}

.tpl .txt1{padding:2px 5px 2px 5px;width:96%;height:36px;line-height:36px;border:1px solid #A6A6A6;font-size:15px;color:#737373;margin:0px;outline:0 none;}
.tpl .txt1:focus{border-color:#ABADB3;box-shadow:0px 0px 3px rgba(0,0,0,0.1);}
.tpl .txt1.intip{color:#D2D2D2;}
.tpl .tpl-login{font-size:13px;}

.tpl .tpl-ct{position:relative;}
.tpl .label1{width:100%;height:100%;line-height:40px;position:absolute;left:0;top:0;color:#D2D2D2;text-indent:10px;background:#fff;}
.tpl .cn-ipt{position:relative;}
.tpl-tb .td1{font-size:15px;}
.tpl-tb .td1{width:65px;}
.tpl-ctl{float:left;}
.tpl-ctr{float:right;}


/*#tpl-addressx*/
#tpl-addressx{padding:2% 5% 2% 5%;width:300px;}
#tpl-addressx .td1{width:80px;text-align:right;color:#333}
#tpl-addressx .td3{color:#E62D00;}
#tpl-addressx  td{height:60px;}
#tpl-addressx .tpl-tips{padding:5px 0px 0px 0px;color:#E62D00;font-size:12px;}
#tpl-addressx .tpl-ac{text-align:center;padding:15px 0px 0px 0px;}
#tpl-addressx .tpl-tb {font-size:16px;}
#tpl-addressx .txt1{background:url(../images/ipt.png) repeat-x;color:#333}
#tpl-addressx .cn-label{padding:3px 0px;}

/*tpl-winx*/
#tpl-winx {width:300px;height:auto;padding:20px 20px 20px 20px;text-align:center;position:relative;font-size:14px;}
#tpl-winx .tpl-tit{height:36px;width:100%;width:126px;position:absolute;top:-25px;margin-left:-63px;left:50%;}
#tpl-winx .tpl-ct1{color:#5D1010;font-size:14px;padding-bottom:15px;}
#tpl-winx .tpl-ct1 b{color:#E62D00}
#tpl-winx .tpl-ct2{color:#5D1010;font-size:14px;padding:7px 0px;border-top:1px dotted #5D1010;}
#tpl-winx .tpl-ct2 .p1{color:#F09E1E;font-size:14px;font-weight:bold;padding-bottom:10px;}
#tpl-winx .tpl-ct2 .p2{}
#tpl-winx .tpl-ct2 .p3{padding:5px 0px 0px 0px;text-align:center;}
#tpl-winx a,#tpl-winx a:link,#tpl-winx a:hover{color:#5D1010;text-decoration:underline;}


/*tpl-noWin*/
#tpl-noWinx {width:300px;padding:20px 20px 35px 20px;text-align:center;position:relative;font-size:14px;}
#tpl-noWinx .tpl-tit{height:36px;width:100%;width:126px;position:absolute;top:-25px;margin-left:-63px;left:50%;}
#tpl-noWinx  .tpl-ct1{color:#5D1010;font-size:16px;padding-bottom:10px;}
#tpl-noWinx .tpl-ct1 b{color:#E62D00}
#tpl-noWinx .tpl-ct2{color:#5D1010;font-size:16px;padding:10px 0px; }
#tpl-noWinx a,#tpl-noWinx  a:link,#tpl-noWinx  a:hover{color:#5D1010;text-decoration:underline;}
#tpl-noWinx .tpl-ct3{padding:10px 0px 15px 0px;color:#F0980F;font-weight:bold;font-size:16px;}
#tpl-noWinx .tpl-ct4 a{display:inline-block;_zoom:1;*display:inline;width:145px;}


/*tpl-wlgx*/
#tpl-wlgx {padding:20px 50px 30px 50px;width:400px;}
#tpl-wlgx .tpl-tips{color:#bf0008;font-size:18px;font-weight:bold;}
#tpl-wlgx .tpl-txt{color:#333;font-size:14px;padding-top:10px;}
#tpl-wlgx .tpl-img{padding-top:15px;text-align:center;}
#tpl-wlgx .tpl-ac{text-align:center;padding-top:20px;}



/*tpl-thanksx*/
