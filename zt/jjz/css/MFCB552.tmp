/*responsive*/
@media only screen and (max-width:720px) {

    .txts,.cn-txt{font-size:22px;}
    .tipsWin{width:90.3125%;height:81.4484127%;left:8.125%;top:6.94444444%;margin-left:3.5%;}
    .tipsWin .tc_clo{width:8.30449827%;height:5.48112058%;top:6.09013398%;}
    .container,.wraper,.nav,.cn-slidetips{width:100%;}
    .cn-slidetips{margin:0 auto;left:0;}
    .container {height:100%;overflow:hidden;}
    .win{
        width:90%;height: auto;
    }
    .cn-win .ctx{font-size:22px;}
   
}

@media only screen and (max-width:540px) {
    .txts,.cn-txt, .cn-win .ctx{font-size:16px;}
 
 

}

@media only screen and (max-width:360px) {
    .txts,.cn-txt, .cn-win .ctx{font-size:14px;}
}

@media only screen and (max-width:320px) {
   .txts,.cn-txt, .cn-win .ctx{font-size:12px;}
    /*page-3*/
    .page-3 .am7 p{font-size:12px;line-height:1.6em;font-weight:normal}
  
}


.animation{display:none;}
.animation.hinge {
	-webkit-animation:2s ease;
	-moz-animation:2s ease;
	-ms-animation:2s ease;
	-o-animation:2s ease;
	animation:2s ease
}
.animation {
	-webkit-animation-duration:1s;
	-webkit-animation-delay:.2s;
	-webkit-animation-timing-function:ease;
	-webkit-animation-fill-mode:both;
	-moz-animation-duration:1s;
	-moz-animation-delay:.2s;
	-moz-animation-timing-function:ease;
	-moz-animation-fill-mode:both
}
@-webkit-keyframes bounceIn {
	0% {
         display:block;
	opacity:0;
	-webkit-transform:scale(.3)
}
50% {
	opacity:1;
	-webkit-transform:scale(1.05)
}
70% {
	-webkit-transform:scale(.9)
}
100% {
	-webkit-transform:scale(1)
}
}
@-moz-keyframes bounceIn {
	0% {
         display:block;
	opacity:0;
	-moz-transform:scale(.3)
}
50% {
	opacity:1;
	-moz-transform:scale(1.05)
}
70% {
	-moz-transform:scale(.9)
}
100% {
	-moz-transform:scale(1)
}
}.bounceIn {
	-webkit-animation-name:bounceIn;
	-moz-animation-name:bounceIn
}
@-webkit-keyframes bounceInUp {
	0% {
         display:block;
	opacity:0;
	-webkit-transform:translateY(2000px)
}
60% {
	opacity:1;
	-webkit-transform:translateY(-30px)
}
80% {
